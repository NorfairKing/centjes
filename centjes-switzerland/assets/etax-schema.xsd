	<xs:complexType name="mutationJournalEntryType">
		<xs:sequence>
			<xs:element name="cantonExtension" type="eCH-0119:cantonExtensionType" minOccurs="0"/>
			<xs:element name="xpath" type="xs:string"/>
			<xs:element name="old" type="xs:string" minOccurs="0"/>
			<xs:element name="new" type="xs:string" minOccurs="0"/>
			<xs:element name="mutationType">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="en">insert</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="en">update</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="en">delete</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="mutationDescription">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="en">retention of profits</xs:documentation>
								<xs:documentation xml:lang="de">Thesaurierung</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="en">add missing value</xs:documentation>
								<xs:documentation xml:lang="de">Ergänzung fehlender Wert</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="en">correction of wrong value</xs:documentation>
								<xs:documentation xml:lang="de">Korrektur falscher Wert</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="4">
							<xs:annotation>
								<xs:documentation xml:lang="en">other</xs:documentation>
								<xs:documentation xml:lang="de">anderer Grund</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="5">
							<xs:annotation>
								<xs:documentation xml:lang="en">automatically updated by the calculation of the application</xs:documentation>
								<xs:documentation xml:lang="de">automatische Aktualisierung der Berechnung durch Applikation</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="mutationText" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cantonExtensionType">
